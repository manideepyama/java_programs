class Circle{
	float r,a;
	Circle(){ r=2.0f; }
	Circle(float r) { this.r=r; }
	float findArea( ){ 
	    a=3.14f*r*r;
		return a; }
   public static void main(String args[])
	  { Circle c=new Circle( ); 
